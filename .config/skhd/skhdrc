
# === launch commands ===
# open kitty terminal, blazingly fast compared to iTerm/Hyper
alt - return : /Applications/Alacritty.app/Contents/MacOS/alacritty 

# open system preferences
hyper - c : open "x-apple.systempreferences:"

alt - b : open -n -a "Brave Browser"
alt - c : open -n -a "Google Chrome"
alt - n : open -a "Finder" 


